n        await this.login({ username, password, url: `${urls.getUrls().services}/platform/auth/login/csa` });\n        \n        await this.initApi();\n        await this.initApp();\n        this.username = '';\n        this.password = '';\n        this.$router.push('/');\n      } catch (e) {\n          console.log(e);\n          if (e.message.includes(\"403\")) {\n              this.loginAlert = \"You do not have the correct permissions to access this app\";\n            } else if (e.message.includes(\"401\")) {\n              this.loginAlert = \"Invalid credentials\";\n            } else {\n            this.loginAlert = \"Unknown error, contact developer\"\n          }\n        \n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}