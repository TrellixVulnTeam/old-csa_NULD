A,GAAA,EAAA;AAAA,OARA,EASA;AAAA,QAAA,KAAA,EAAA,YAAA;AAAA,QAAA,GAAA,EAAA;AAAA,OATA,EAUA;AAAA,QAAA,KAAA,EAAA,YAAA;AAAA,QAAA,GAAA,EAAA;AAAA,OAVA,EAWA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,GAAA,EAAA;AAAA,OAXA,EAYA;AAAA,QAAA,KAAA,EAAA,UAAA;AAAA,QAAA,GAAA,EAAA;AAAA,OAZA,CAFA;AAgBA,MAAA,OAAA,EAAA,EAhBA;AAiBA,MAAA,WAAA,EAAA,CAjBA;AAkBA,MAAA,SAAA,EAAA,QAAA,CAAA;AAlBA,KAAA;AAoBA,GA7BA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA,CAAA,CADA;AAEA,IAAA,YAFA,0BAEA;AACA,UAAA,KAAA,KAAA,CAAA,CAAA,EAAA,QAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,KAAA,EAAA,WAAA;AAAA,UAAA,GAAA,EAAA;AAAA,SAAA;AACA,OAFA,MAEA,IAAA,KAAA,KAAA,CAAA,CAAA,EAAA,UAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,KAAA,EAAA,SAAA;AAAA,UAAA,GAAA,EAAA;AAAA,SAAA;AACA,OAFA,MAEA,IAAA,KAAA,KAAA,CAAA,CAAA,EAAA,SAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,KAAA,EAAA,QAAA;AAAA,UAAA,GAAA,EAAA;AAAA,SAAA;AACA;AACA,KAVA;AAWA,IAAA,YAXA,wBAWA,IAXA,EAWA,KAXA,EAWA;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,KAAA,KAAA,EAAA,KAAA,GAAA,KAAA,OAAA,IAAA,KAAA,WAAA,GAAA,CAAA,CAAA,kCAAA,IAAA;AAAA,QAAA,YAAA,EAAA,CAAA,IAAA,CAAA;AAAA;AACA;AAbA,GA9BA;AA8CA,EAAA,OA9CA,qBA8CA;AACA,SAAA,YAAA;AACA,GAhDA;AAiDA,EAAA,QAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,aAAA,KAAA,KAAA,CAAA,MAAA,GAAA,KAAA,OAAA;AACA;AAHA,GAjDA;AAsDA,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA,oBAAA,KAAA,EAAA;AACA,aAAA,IAAA,IAAA,CAAA,KAAA,EAAA,cAAA,EAAA;AACA;AAHA;AAtDA,CAAA","sourcesContent":["<template>\n    <div>\n\n\n        <b-input-group\n            class=\"mt-5 mb-3\"\n            prepend=\"Filter\"\n        >\n            <b-form-input\n                v-model=\"filter\"\n            ></b-form-input>\n        </b-input-group>\n\n        <b-dropdown\n            variant=\"outline-dark\"\n            class=\"mb-3 float-right\"\n            :text=\"'Entries per Page: ' + perPage\"\n            size=\"sm\"\n        >\n            <b-dropdown-item\n                v-for=\"entryOption in [\n                    5, 10, 25, 100,\n                ]\"\n                @click=\"perPage = entryOption\"\n                :key=\"entryOption\"\n                >{{ entryOption }}</b-dropdown-item\n            >\n        </b-dropdown>\n        <b-table\n            :filter=\"filter\"\n            selectable\n            select-mode=\"single\"\n            @row-selected=\"onRowSelected\"\n            striped\n            hover\n            bordered\n            head-variant=\"dark\"\n            class=\"w-100 border\"\n            :items=\"items\"\n            :fields=\"fields\"\n            :per-page=\"perPage\"\n            :current-page=\"currentPage\"\n        >\n            <template v-slot:head()=\"data\">\n                <h5 class=\"m-0\">\n                    {{ data.label }}\n                </h5>\n            </template>\n\n            <template #cell(fullName)=\"{ item }\">\n                <div>{{ item.firstname }} {{ item.lastname }}</div>\n            </template>\n\n            <template #cell(roleID)=\"{ value }\">\n                <div>\n                    {{ userTypes[value] }}\n                </div>\n            </template>\n\n            <template #cell(passwordClear)=\"{ item, index }\">\n                <div>\n                    <b-input-group>\n                        <b-form-input\n                            class=\"text-right\"\n                            :type=\"item.showPassword ? 'text' : 'password'\"\n                            plaintext\n                            :value=\"item.passwordClear\"\n                        ></b-form-input>\n\n                        <b-button variant=\"white\" @click=\"showPassword(item, index)\" v-if=\"item.passwordClear\">\n                            <b-icon :icon=\"item.showPassword ? 'eye' : 'eye-slash'\"></b-icon>\n                        </b-button>\n                    </b-input-group>\n                </div>\n            </template>\n\n            <template #cell(email)=\"{ value }\">\n                <div>\n                    <p v-if=\"value\">{{value}}</p>\n                    <b-icon v-else icon=\"dash\"></b-icon>\n                </div>\n            </template>\n\n            <template #cell(isSubscribedToEmails)=\"{ value }\">\n                <div>\n                    <b-icon\n                        v-if=\"value\"\n                        icon=\"check\"\n                        font-scale=\"1.5\"\n                        variant=\"success\"\n                    ></b-icon>\n                    <b-icon\n                        v-else\n                        icon=\"x-circle\"\n                        font-scale=\"1.5\"\n                        variant=\"danger\"\n                    ></b-icon>\n                </div>\n            </template>\n\n            <template #cell(lastLoginOn)=\"{ value }\">\n                <div>\n                    {{ value | toDateTime }}\n                </div>\n            </template>\n\n            <template #cell(isDeleted)=\"{ value }\">\n                <div>\n                    <b-icon\n                        v-if=\"value\"\n                        icon=\"check\"\n                        font-scale=\"1.5\"\n                        variant=\"danger\"\n                    ></b-icon>\n                    <b-icon v-else icon=\"dash\"></b-icon>\n                </div>\n            </template>\n\n\n            <template #cell(products)=\"{ item }\">\n                <div>\n                    <Logo v-if=\"item.reflexLicense\" product=\"reflex\"></Logo>\n                    <Logo v-if=\"item.fractionsLicense\" product=\"frax\"></Logo>\n                    <Logo v-if=\"item.gizmosLicense\" product=\"gizmos\"></Logo>\n                    <Logo v-if=\"item.s4usLicense\" product=\"s4us\"></Logo>\n                </div>\n            </template>\n        </b-table>\n        <b-pagination\n            v-if=\"showPagination\"\n            size=\"md\"\n            class=\"w-50 m-auto\"\n            v-model=\"currentPage\"\n            :total-rows=\"items.length\"\n            :per-page=\"perPage\"\n            aria-controls=\"my-table\"\n            align=\"fill\"\n        ></b-pagination>\n    </div>\n</template>\n\n<script>\nimport usersInUmbrella from \"../misc/usersInUmbrella.json\";\nimport csaTypes from \"../misc/csaTypes\";\nimport Logo from './WrapperComponents/Logo.vue';\nimport Vue from 'vue';\n\nexport default {\n    components: { Logo },\n    props: {\n        filter: {\n            type: String,\n            default: \"\",\n        },\n    },\n    data() {\n        return {\n            items: usersInUmbrella.ArrayList,\n            fields: [\n                { label: \"Full Name\", key: \"fullName\" },\n                { label: \"Institution\", key: \"institutionName\" },\n                { label: \"Login ID\", key: \"loginID\" },\n                { label: \"Role\", key: \"roleID\", sortable:true },\n                { label: \"Username\", key: \"username\" },\n                { label: \"Password\", key: \"passwordClear\" },\n                { label: \"Student ID\", key: \"sisUserID\" },\n                { label: \"Email\", key: \"email\" },\n                { label: \"Subscribed\", key: \"isSubscribedToEmails\" },\n                { label: \"Last Login\", key: \"lastLoginOn\" },\n                { label: \"Deleted\", key: \"isDeleted\" },\n                { label: \"Products\", key: \"products\" },\n            ],\n            perPage: 10,\n            currentPage: 1,\n            userTypes: csaTypes.loginRoleAllOptions,\n        };\n    },\n    methods: {\n        onRowSelected() {},\n        onDataFilled() {\n            if (this.items[0].cleverID) {\n                this.fields.push({ label: \"Clever ID\", key: \"cleverID\" });\n            } else if (this.items[0].samlUserID) {\n                this.fields.push({ label: \"SAML ID\", key: \"samlUserID\" });\n            } else if (this.items[0].ltiUserID) {\n                this.fields.push({ label: \"LTI ID\", key: \"ltiUserID\" });\n            }\n        },\n        showPassword(item, index) {\n            Vue.set(this.items,index+(this.perPage* (this.currentPage-1)), {...item, showPassword: !item.showPassword});\n        }\n        \n    },\n    created() {\n        this.onDataFilled();\n    },\n    computed: {\n        showPagination() {\n            return this.items.length > this.perPage;\n        },\n    },\n    filters: {\n        toDateTime: function(value) {\n            return new Date(value).toLocaleString();\n        }\n    },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n</style>"],"sourceRoot":"src/components"}]}