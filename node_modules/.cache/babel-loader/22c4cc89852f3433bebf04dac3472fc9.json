CA,MAAA,gBAAA,CAAA,OAAA,CAAA,cAAA,GACA,KAAA,CAAA,CAAA,CAAA,CAAA,cAAA,CAAA,UAAA,CAAA,GAAA,EAAA,IAAA,CADA;AAEA,WAAA,oBAAA,GAAA,gBAAA,CAAA,OAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,IAAA;AACA,KAdA;AAeA,IAAA,eAfA,2BAeA,OAfA,EAeA;AACA,UAAA,OAAA,GAAA,KAAA,KAAA,CAAA,OAAA,CAAA,CADA,CACA;;AAEA,UAAA,KAAA;;AACA,UAAA,QAAA,CAAA,SAAA,EAAA;AACA;AACA,QAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,QAAA,KAAA,CAAA,iBAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,MAAA;AACA,OALA,MAKA,IAAA,MAAA,CAAA,YAAA,EAAA;AACA,QAAA,KAAA,GAAA,QAAA,CAAA,WAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,eAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,QAAA,CAAA,KAAA;AACA;;AACA,MAAA,QAAA,CAAA,WAAA,CAAA,MAAA;AACA;AA/BA,GAvDA;AAwFA,EAAA,QAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,aAAA,KAAA,KAAA,CAAA,MAAA,GAAA,KAAA,OAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AACA,UAAA,QAAA,GAAA,gBAAA,CAAA,OAAA,CAAA,cAAA;AACA,UAAA,SAAA,GAAA,KAAA,oBAAA,CAAA,OAAA,GAAA,iBAAA,GAAA,SAAA;AACA,UAAA,aAAA,GAAA,QAAA,CAAA,oBAAA,CAAA,KAAA,oBAAA,CAAA,eAAA,CAAA;AAEA,UAAA,IAAA,GAAA,qBAAA,QAAA,GAAA,SAAA,GAAA,KAAA,oBAAA,CAAA,qBAAA,GAAA,aAAA,GAAA,YAAA;;AAEA,UAAA,KAAA,gBAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,QAAA,IAAA,IAAA,QAAA,KAAA,cAAA,CAAA,sBAAA,CAAA,CAAA,EAAA,eAAA;AACA,OAFA,MAGA,IAAA,KAAA,YAAA,IAAA,KAAA,YAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,QAAA,IAAA,IAAA,OAAA,KAAA,YAAA,CAAA,CAAA,EAAA,eAAA;AACA,OAFA,MAGA;AACA,QAAA,IAAA,IAAA,QAAA,KAAA,oBAAA,CAAA,iBAAA;AACA;;AACA,aAAA,IAAA;AACA;AArBA;AAxFA,CAAA","sourcesContent":["<template>\n    <div>\n        <!-- Page Title -->\n        <div>\n            <h1 class=\"m-0 text-center\">Orders Dashboard</h1>\n        </div>\n\n        <b-container fluid>\n            <b-row>\n                <b-col cols=\"9\">\n                    <b-card class=\"card shadow m-4 text-center text-dark\">\n                        <b-card-text>\n                            <slot name=\"card-body\">\n                                <!-- Draft Orders -->\n                                <b-row>\n                                    <b-col>\n                                        <h2 class=\"text-left m-0\">\n                                            <b-button\n                                                @click=\"\n                                                    chevronDraft = !chevronDraft\n                                                \"\n                                                variant=\"white\"\n                                            >\n                                                <b-icon\n                                                    :icon=\"\n                                                        collapseChevron(\n                                                            chevronDraft\n                                                        )\n                                                    \"\n                                                    font-scale=\"2\"\n                                                    class=\"m-0 p-0\"\n                                                ></b-icon\n                                            ></b-button>\n                                            Draft Orders (10)\n                                        </h2>\n                                    </b-col>\n                                    <b-col>\n                                        <b-input-group\n                                            class=\"\"\n                                            prepend=\"Filter\"\n                                            :hidden=\"!chevronDraft\"\n                                        >\n                                            <b-form-input\n                                                v-model=\"draftFilter\"\n                                            ></b-form-input>\n                                        </b-input-group>\n                                    </b-col>\n                                </b-row>\n                                <b-collapse\n                                    id=\"draftOrders\"\n                                    v-model=\"chevronDraft\"\n                                    class=\"center\"\n                                >\n                                    <b-dropdown\n                                        variant=\"outline-dark\"\n                                        class=\"mb-3 float-right\"\n                                        :text=\"'Entries per Page: ' + perPage\"\n                                        size=\"sm\"\n                                    >\n                                        <b-dropdown-item\n                                            v-for=\"entryOption in [\n                                                5, 10, 25, 100,\n                                            ]\"\n                                            @click=\"perPage = entryOption\"\n                                            :key=\"entryOption\"\n                                            >{{ entryOption }}</b-dropdown-item\n                                        >\n                                    </b-dropdown>\n                                    <b-table\n                                        :filter=\"draftFilter\"\n                                        selectable\n                                        select-mode=\"single\"\n                                        @row-selected=\"onOrderSelected\"\n                                        striped\n                                        hover\n                                        bordered\n                                        head-variant=\"dark\"\n                                        :items=\"items\"\n                                        :fields=\"fields\"\n                                        class=\"w-100 border\"\n                                        :per-page=\"perPage\"\n                                        :current-page=\"currentPage\"\n                                    ><template v-slot:head()=\"data\">\n                                            <h5 class=\"m-0\">\n                                                {{ data.label }}\n                                            </h5>\n                                        </template>\n                                        <template #cell(productsString)=\"data\">\n\n                                            <Logo\n                                                class=\"m-1\"\n                                                v-for=\"prod in data.value.split(';')\"\n                                                :key=\"prod\"\n                                                style=\"height: 40px\"\n                                                version=\"leaf\"\n                                                :product=\"prod.toLowerCase()\"\n                                            ></Logo>\n                                        </template>\n                                        <template #cell(actions)=\"\">\n                                            <b-button\n                                                variant=\"outline-success\"\n                                                class=\"mr-1\"\n                                            >\n                                                Provision\n                                            </b-button>\n                                            <b-button variant=\"outline-info\">\n                                                Umbrella\n                                            </b-button>\n                                        </template>\n                                    </b-table>\n                                    <b-pagination\n                                        v-if=\"showPagination\"\n                                        size=\"md\"\n                                        class=\"w-50 m-auto\"\n                                        v-model=\"currentPage\"\n                                        :total-rows=\"items.length\"\n                                        :per-page=\"perPage\"\n                                        aria-controls=\"my-table\"\n                                        align=\"fill\"\n                                    ></b-pagination>\n                                </b-collapse>\n                            </slot>\n                        </b-card-text>\n                    </b-card>\n                </b-col>\n                <b-col cols=\"3\">\n                    <b-card class=\"shadow my-4 mr-4 p-0\">\n                        <b-card-text>\n                            <h2>Overview</h2>\n                            <b-card class=\"p-0\" body-class=\"p-0\">\n                                <b-card-text class=\"p-0\">\n                                    <div>\n                                        <b-alert\n                                            show\n                                            variant=\"secondary\"\n                                            class=\"rounded-0 m-0\"\n                                            ><h4 class=\"m-0\">\n                                                Activated Orders\n                                            </h4>\n                                        </b-alert>\n                                    </div>\n                                    <div class=\"p-2\">\n                                        <p class=\"m-0 font-weight-bold\">\n                                            Today\n                                        </p>\n                                        <b-row>\n                                            <b-col>\n                                                <p class=\"display-4 m-0\">13</p>\n                                                <p>New</p>\n                                            </b-col>\n                                            <b-col>\n                                                <p class=\"display-4 m-0\">5</p>\n                                                <p>Renewal</p>\n                                            </b-col>\n                                        </b-row>\n                                    </div>\n                                    <OrderChart></OrderChart>\n                                </b-card-text>\n                            </b-card>\n                        </b-card-text>\n                    </b-card>\n                </b-col>\n            </b-row>\n            <b-row>\n                <b-col>\n                <b-card class=\"card shadow m-4 text-center text-dark\">\n                            <h2>Email History of last 24hrs</h2>\n                    <b-card-text>\n                    <email-history></email-history>\n                    </b-card-text>\n                </b-card>\n                </b-col>\n            </b-row>\n        </b-container>\n\n        <!-- Order Modal -->\n\n        <b-modal size=\"xl\" ref=\"order-modal\" no-close-on-backdrop no-footer>\n            <template #modal-title>\n                <h1 class=\"m-0 ml-3\">\n                    Order # {{ selectedOrderDetails.salesforceOrderNumber }}\n                </h1>\n            </template>\n            <b-container>\n                <b-row>\n                    <b-col>\n                        <b-card class=\"text-center\">\n                            <OrderSummary :selectedOrderDetails=\"selectedOrderDetails\"></OrderSummary>\n                        </b-card>\n                    </b-col>\n                    <b-col cols=\"7\" v-if=\"provisionProcess === 1\">\n                        <b-card title=\"Order Umbrella\" class=\"mb-4\">\n                            <b-text>\n                                <b-row>\n                                    <b-col>\n                                        <strong>Parent Institution Name</strong>\n                                        <p>Parent Institution</p>\n                                    </b-col>\n                                    <b-col>\n                                        <strong>non Site Subscriptions</strong>\n                                        <p>Grant</p>\n                                    </b-col>\n                                    <b-col cols=\"3\">\n                                        <strong>Umbrella ID</strong>\n                                        <p>520314</p>\n                                    </b-col>\n                                    <b-col>\n                                        <b-button\n                                            class=\"float-right\"\n                                            variant=\"success\"\n                                            >Provision\n                                            <b-icon\n                                                style=\"vertical-align: sub\"\n                                                icon=\"arrow-right\"\n                                                class=\"ml-2\"\n                                            ></b-icon\n                                        ></b-button>\n                                    </b-col>\n                                </b-row>\n                            </b-text>\n                        </b-card>\n\n                        <small class=\"text-secondary\">Other possible Umbrellas (Default search by PID):</small>\n                        <b-input-group prepend=\"Find Umbrella\" class=\"my-3\">\n                            <b-form-input placeholder=\"02055386\"></b-form-input>\n                            <b-input-group-append>\n                                <b-button variant=\"outline-success\"\n                                    >Search</b-button\n                                >\n                            </b-input-group-append>\n                        </b-input-group>\n                        <b-table\n                            :items=\"possibleUmbrellas\"\n                            :fields=\"[\n                                'parentInstitutionName',\n                                'nonSiteSubscription',\n                                'umbrellaID',\n                                'action',\n                            ]\"\n                        >\n                            <template #cell(nonSiteSubscription)>\n                                Grant, Trial\n                            </template>\n                            <template #cell(action)>\n                                <b-button variant=\"outline-success\"\n                                    >Provision\n                                    <b-icon\n                                        style=\"vertical-align: sub\"\n                                        icon=\"arrow-right\"\n                                        class=\"ml-2\"\n                                    ></b-icon>\n                                </b-button>\n                            </template>\n                        </b-table>\n                        <small class=\"text-secondary\"\n                            >Umbrella doesn't exist?<b-button\n                                class=\"ml-2\" size=\"sm\"\n                                variant=\"outline-success\"\n                                >Create one</b-button\n                            ></small\n                        >\n                    </b-col>\n                    <b-col  cols=\"7\"  v-if=\"provisionProcess === 2\">\n                            <h2 class=\"text-center mt-3\">Order item 1 of 2</h2>\n                            <Logo style=\"height:40px\" class=\"d-block mx-auto mt-3\" version=\"full\" product=\"reflex\"></Logo>\n\n                        <ViewEditSubscription class=\"mt-3\" :subscriptionData=\"subscriptionData\"></ViewEditSubscription>\n                    </b-col>\n                    <b-col  cols=\"7\"  v-if=\"provisionProcess === 3\">\n                        <h2 class=\"text-center mt-3\">Order Confirmation Email</h2>\n\n                        <CSAEmail class=\"mt-4\" :toEmail=\"selectedOrderDetails.schoolContactEmail\" :ccEmail=\"selectedOrderDetails.orderOwnerEmail\" :title=\"emailSubject\" :isRenewal=\"selectedOrderDetails.renewal\"></CSAEmail>\n                    </b-col>\n\n                </b-row>\n            </b-container>\n            <template #modal-footer=\"{ cancel }\">\n                <div>\n                    <b-button @click=\"cancel()\">Cancel</b-button>\n                </div>\n            </template>\n        </b-modal>\n    </div>\n</template>\n\n<script>\n    import OrdersData from \"../misc/ordersData.json\";\n    import OrderDetailsData from \"../misc/orderDetails.json\";\n    import Logo from \"../components/WrapperComponents/Logo.vue\";\nimport OrderChart from '../components/Order/OrderChart.vue';\nimport ViewEditSubscription from '../components/ViewEditSubscription.vue';\nimport CSAEmail from '../components/CSAEmail.vue';\nimport csaTypes from '../misc/csaTypes'\nimport OrderSummary from '../components/Order/OrderSummary.vue';\nimport EmailHistory from '../components/EmailHistory.vue';\n    export default {\n        components: { Logo, OrderChart, ViewEditSubscription, CSAEmail, OrderSummary,\n                EmailHistory,\n         },\n        props: {\n            bodyVisible: {\n                type: Boolean,\n                default: true,\n            },\n        },\n        data() {\n            return {\n                provisionProcess: 3,\n                possibleUmbrellas: [\n                    {\n                        umbrellaID: 100,\n                        parentInstitutionName: \"Fake institution name\",\n                    },\n                ],\n                draftFilter: \"\",\n                activeFilter: \"\",\n                items: OrdersData.reduce((items, item) => {\n                    if (\n                        items.find(\n                            (x) =>\n                                x.salesforceOrderNumber ===\n                                item.salesforceOrderNumber\n                        )\n                    ) {\n                        return items;\n                    } else {\n                        items.push(item);\n                        return items;\n                    }\n                }, []),\n                fields: [\n                    {\n                        key: \"salesforceOrderNumber\",\n                        label: \"Order Number\",\n                        sortable: true,\n                    },\n                    { key: \"orderName\", label: \"Order Name\", sortable: true },\n                    { key: \"orderOwner\", label: \"Sales Rep.\", sortable: true },\n                    { key: \"productsString\", label: \"Products\", sortable: true },\n                ],\n                chevronDraft: true,\n                chevronActive: false,\n                currentPage: 1,\n                perPage: 5,\n                selectedOrderDetails: {},\n                subscriptionData: {\"Subscription\":{\"modifiedByID\":0,\"modifiedOn\":\"2016-09-16T18:57:17.350Z\",\"createdByID\":0,\"createdOn\":\"2016-09-12T16:08:30.656666700Z\",\"subscriptionName\":\"Emery, Marianne\",\"licensingTypeID\":8,\"productID\":1,\"salesForceContractID\":null,\"salesForceContractNumber\":null,\"startsOn\":\"2016-09-12T04:00:00Z\",\"expiresOn\":\"2017-10-16T04:00:00Z\",\"enforceStartDate\":false,\"gracePeriod\":0,\"isActive\":true,\"tenantID\":2029,\"pendingSubscriptionID\":0,\"pendingEffectiveOn\":null,\"sourceID\":0,\"notes\":null,\"isSSOLTIEnabled\":false,\"isSSOSamlEnabled\":false,\"isRosterEnabled\":true,\"teachersCap\":0,\"teachersUsed\":0,\"upgradeAllowed\":true,\"extraSubscriptionAccessJson\":null,\"lengthInDays\":399,\"isExpired\":true,\"subscriptionMemberList\":[{\"modifiedByID\":0,\"modifiedOn\":null,\"createdByID\":0,\"createdOn\":null,\"subscriptionID\":145426,\"institutionID\":68294,\"seatsCap\":35,\"seatsUsed\":18,\"teachersCap\":0,\"teachersUsed\":0,\"tenantID\":2029,\"institutionTypeID\":4,\"institutionName\":\"BAYWOOD ELEMENTARY SCHOOL\",\"parentInstitutionID\":2498,\"parentInstitutionName\":\"SAN LUIS COASTAL UNIF SCH DIST\",\"stateCode\":\"CA\",\"countryCode\":\"USA\",\"city\":\"LOS OSOS\",\"enrollment\":344,\"allowRolesToEnrollStudents\":false,\"startOfSchoolDay\":22,\"startOfSchoolMonth\":8,\"defaultLanguageID\":0,\"externalSchoolID\":null}]}}.Subscription,\n                institutions:[],\n                subscriptionList:[],\n            };\n        },\n        methods: {\n            openInNewTab(url) {\n                window.open(url);\n            },\n            collapseChevron(id) {\n                return id ? \"dash-circle\" : \"plus-circle\";\n            },\n            onOrderSelected(order) {\n                // console.log(order);\n                OrderDetailsData.SFOrder.productsString =\n                    order[0].productsString.replaceAll(\";\", \", \");\n                this.selectedOrderDetails = OrderDetailsData.SFOrder;\n                console.log(OrderDetailsData);\n                this.$refs[\"order-modal\"].show();\n            },\n            copyToClipboard(refName) {\n                const element = this.$refs[refName]; // e.g. <div ref=\"text\">\n\n                var range;\n                if (document.selection) {\n                    // IE\n                    range = document.body.createTextRange();\n                    range.moveToElementText(element);\n                    range.select();\n                } else if (window.getSelection) {\n                    range = document.createRange();\n                    range.selectNode(element);\n                    window.getSelection().removeAllRanges();\n                    window.getSelection().addRange(range);\n                }\n                document.execCommand(\"copy\");\n            },\n        },\n        computed: {\n            showPagination() {\n                return this.items.length > this.perPage;\n            },\n            emailSubject() {\n                const products = OrderDetailsData.SFOrder.productsString;\n                const orderType = this.selectedOrderDetails.renewal ? \" Renewal Order \" : \" Order \";\n                const licensingType = csaTypes.licensingTypeOptions[this.selectedOrderDetails.licensingTypeID];\n\n                var name =  \"ExploreLearning \" + products + orderType + this.selectedOrderDetails.salesforceOrderNumber + licensingType + \" LICENSE #\";\n\n                if (this.subscriptionList.length == 1) {\n                    name += \" - \" + this.urSubscription.subscriptionMemberList[0].institutionName;\n                } \n                else if (this.institutions && this.institutions.length == 1) {\n                    name += \"- \" + this.institutions[0].institutionName;\n                }\n                else {\n                    name += \" - \" + this.selectedOrderDetails.schoolContactName;\n                }\n                return name;\n            }\n        },\n    };\n</script>\n<style lang=\"scss\" scoped>\n    .modal-dialog {\n        width: 100%;\n        margin: 0;\n    }\n    p {\n        margin: 0;\n    }\n</style>\n\n"],"sourceRoot":"src/views"}]}