zmoGreen],\n            },\n        ],\n    };\n    const config = {\n        type: \"pie\",\n        data: data,\n        options: {\n            responsive: true,\n            plugins: {\n                legend: {\n                    position: \"top\",\n                },\n            },\n        },\n    };\n\n    export default {\n        data() {\n            return {\n                myChart: {},\n            };\n        },\n        methods: {\n            createSSOPie() {\n                var ctx = document.getElementById(\"productChart\");\n\n                this.myChart = new Chart(ctx, config);\n            },\n        },\n        mounted() {\n            this.createSSOPie();\n        },\n        destroyed() {\n            console.log(\"DESTROYING CHART\");\n            this.myChart.destroy();\n        },\n    };\n</script>\n\n<style lang=\"scss\" scoped>\n</style>"]}]}